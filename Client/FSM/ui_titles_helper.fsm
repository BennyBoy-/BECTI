       "	_txt = """";" \n
       "	_lastCheck = ""Camp"";" \n
       "} else {" \n
       "	_txt = Format [""Town SV: %1/%2"",_curSV,_maxSV];" \n
       "	_lastCheck = ""Town"";" \n
       "};" \n
       "" \n
